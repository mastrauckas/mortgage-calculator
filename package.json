{
  "name": "mortgage-playground",
  "version": "1.0.0",
  "description": "Mortgage Playground website",
  "scripts": {
    "build": "npm run build:dev",
    "build:dev": "npm run build:dev:watch",
    "build:dev:watch": "npm run clean && cross-env NODE_ENV=development webpack --progress --profile --color --watch",
    "build:dev:nowatch": "npm run clean && cross-env NODE_ENV=development webpack --progress --profile --color",
    "build:prod": "npm run clean c && cross-env NODE_ENV=production webpack --progress --profile --color",
    "dev-server": "cd ./src/cs/ && cross-env ASPNETCORE_ENVIRONMENT=Development dotnet run",
    "web-server": "npm run lint && cross-env NODE_ENV=development webpack-dev-server --inline --progress",
    "test": "npm run test:cs:endpoints && npm run test:cs:models",
    "test:cs:endpoints": "cd ./tests/cs/Maa.MortgageCalculator.Endpoints.Tests &&  dotnet test",
    "test:cs:models": "cd ./tests/cs/Maa.MortgageCalculator.Models.Tests/ && dotnet test",
    "lint": "npm run lint:javascript",
    "lint:javascript": "eslint src",
    "clean": "rimraf ./build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mastrauckas/mortgage-playground.git"
  },
  "author": "Michael Astrauckas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mastrauckas/mortgage-playground.git/issues"
  },
  "homepage": "https://github.com/mastrauckas/MortgagePlayground#readme",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.13.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2016": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.4",
    "eslint": "^3.16.0",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-react": "^6.10.0",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-loader": "^0.5.0",
    "html-webpack-plugin": "^2.28.0",
    "style-loader": "^0.18.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.6.1"
  },
  "dependencies": {
    "axios": "^0.16.0",
    "core-js": "^2.5.0",
    "flux": "^3.1.2",
    "material-ui": "^0.17.4",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-flexbox-grid": "^1.1.2",
    "react-tap-event-plugin": "^2.0.1",
    "sugar": "^2.0.4"
  }
}
