{
  "name": "mortgage-calculator",
  "version": "1.0.0",
  "description": "Mortgage Calculator website",
  "scripts": {
    "restore": "cd ./src/cs/Maa.MortgageCalculator.Endpoints && dotnet restore",
    "dotnet-build": "cd ./src/cs/Maa.MortgageCalculator.Endpoints && dotnet build",
    "build": "yarn build:dev",
    "build:dev": "yarn build:dev:watch",
    "build:dev:watch": "yarn clean && cross-env NODE_ENV=development webpack --progress --profile --color --watch",
    "build:dev:nowatch": "yarn clean && cross-env NODE_ENV=development webpack --progress --profile --color",
    "build:prod": "yarn clean c && cross-env NODE_ENV=production webpack --progress --profile --color",
    "web-server": "cd ./src/cs/Maa.MortgageCalculator.Endpoints && cross-env ASPNETCORE_ENVIRONMENT=Development dotnet run",
    "dev-server:quiet": "yarn lint && cross-env NODE_ENV=development webpack-dev-server --inline",
    "dev-server": "yarn lint && cross-env NODE_ENV=development webpack-dev-server --inline --progress",
    "all-servers": "concurrently --kill-others --names \"DEV-SERVER,WEB-SERVER\" -c \"bgBlue,bgGreen\" \"yarn dev-server:quiet\" \"yarn web-server\"",
    "test": "yarn test:cs:endpoints && yarn test:cs:models",
    "test:cs:endpoints": "cd ./tests/cs/Maa.MortgageCalculator.Endpoints.Tests &&  dotnet test",
    "test:cs:models": "cd ./tests/cs/Maa.MortgageCalculator.Models.Tests/ && dotnet test",
    "lint": "yarn lint:javascript",
    "lint:javascript": "eslint src",
    "clean": "rimraf ./build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mastrauckas/mortgage-calculator.git"
  },
  "author": "Michael Astrauckas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mastrauckas/mortgage-calculator.git/issues"
  },
  "homepage": "https://github.com/mastrauckas/MortgageCalculator#readme",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.13.2",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2016": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "concurrently": "^3.5.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "eslint": "^4.6.1",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-react": "^7.3.0",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.1",
    "html-loader": "^0.5.0",
    "html-webpack-plugin": "^2.28.0",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.6.1"
  },
  "dependencies": {
    "axios": "^0.16.0",
    "core-js": "^2.5.0",
    "flux": "^3.1.2",
    "material-ui": "^0.19.1",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-flexbox-grid": "^1.1.2",
    "react-tap-event-plugin": "^2.0.1",
    "sugar": "^2.0.4"
  }
}
