{
  "name": "mortgage-playground",
  "version": "1.0.0",
  "description": "Mortgage Playground website",
  "scripts": {
    "build": "npm run build:dev",
    "build:dev": "npm run build:dev:watch",
    "build:dev:watch": "npm run clean && npm run mv-py && cross-env NODE_ENV=development webpack --progress --profile --color --watch",
    "build:dev:nowatch": "npm run clean && npm run mv-py && cross-env NODE_ENV=development webpack --progress --profile --color",
    "build:prod": "npm run clean c && cross-env NODE_ENV=production webpack --progress --profile --color",
    "dev-server": "npm run lint && FLASK_APP=./build/app.py FLASK_DEBUG=1 flask run",
    "test": "npm run test:python",
    "test:python": "cross-env PYTHONPATH=./src/api:./src/wwwroot py.test",
    "lint": "npm run lint:python && npm run lint:javascript",
    "lint:python": "flake8 ./src/py",
    "lint:javascript": "eslint src",
    "clean": "rimraf ./build",
    "mv-py": "cpx './src/py/*.py' ./build/ -C"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mastrauckas/mortgage-playground.git"
  },
  "author": "Michael Astrauckas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mastrauckas/mortgage-playground.git/issues"
  },
  "homepage": "https://github.com/mastrauckas/MortgagePlayground#readme",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.13.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.5",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2016": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "cpx": "^1.5.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.27.1",
    "del-cli": "^0.2.1",
    "eslint": "^3.16.0",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "npm-run-all": "^3.1.1",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "flux": "^3.1.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "sugar": "^2.0.4"
  }
}
